class Main {
    static Integer main(Array<String> args) {
        var f = File.open(args[0], 0);
        var stdout = new File(1);
        UnsignedLong bufsz = 4096;
        var buf = new Byte[bufsz];
        UnsignedLong nread;
        while ((nread = f.read(buf, bufsz)) != 0) {
            stdout.write(buf, nread);
        }
        f.close();
        delete f;
        delete stdout;
        delete buf._elements;
        delete buf;
        return 0;
    }
}
